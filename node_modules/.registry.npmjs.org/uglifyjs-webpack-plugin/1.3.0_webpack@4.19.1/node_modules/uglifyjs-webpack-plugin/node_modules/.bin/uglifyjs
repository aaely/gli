#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/3.3.9/node_modules/uglify-es/bin/uglifyjs/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/3.3.9/node_modules/uglify-es/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/3.3.9/node_modules/uglify-es/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/3.3.9/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../uglify-es/3.3.9/node_modules/uglify-es/bin/uglifyjs" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/3.3.9/node_modules/uglify-es/bin/uglifyjs/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/3.3.9/node_modules/uglify-es/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/3.3.9/node_modules/uglify-es/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/3.3.9/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/uglify-es/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../uglify-es/3.3.9/node_modules/uglify-es/bin/uglifyjs" "$@"
  ret=$?
fi
exit $ret

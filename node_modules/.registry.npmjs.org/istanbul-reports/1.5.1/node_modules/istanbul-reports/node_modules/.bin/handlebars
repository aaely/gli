#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/4.4.3/node_modules/handlebars/bin/handlebars/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/4.4.3/node_modules/handlebars/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/4.4.3/node_modules/handlebars/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/4.4.3/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../handlebars/4.4.3/node_modules/handlebars/bin/handlebars" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/4.4.3/node_modules/handlebars/bin/handlebars/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/4.4.3/node_modules/handlebars/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/4.4.3/node_modules/handlebars/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/4.4.3/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/handlebars/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../handlebars/4.4.3/node_modules/handlebars/bin/handlebars" "$@"
  ret=$?
fi
exit $ret

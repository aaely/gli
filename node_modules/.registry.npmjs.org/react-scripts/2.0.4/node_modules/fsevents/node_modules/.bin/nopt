#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/nopt/bin/nopt.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/nopt/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/nopt/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/nopt/bin/nopt.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/nopt/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules/nopt/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules/fsevents/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/1.2.4/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/fsevents/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../nopt/bin/nopt.js" "$@"
  ret=$?
fi
exit $ret

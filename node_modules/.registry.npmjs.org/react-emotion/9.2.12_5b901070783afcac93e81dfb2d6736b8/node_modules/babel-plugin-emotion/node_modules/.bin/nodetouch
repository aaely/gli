#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/2.0.2/node_modules/touch/bin/nodetouch.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/2.0.2/node_modules/touch/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/2.0.2/node_modules/touch/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/2.0.2/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../touch/2.0.2/node_modules/touch/bin/nodetouch.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/2.0.2/node_modules/touch/bin/nodetouch.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/2.0.2/node_modules/touch/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/2.0.2/node_modules/touch/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/2.0.2/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/touch/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../touch/2.0.2/node_modules/touch/bin/nodetouch.js" "$@"
  ret=$?
fi
exit $ret

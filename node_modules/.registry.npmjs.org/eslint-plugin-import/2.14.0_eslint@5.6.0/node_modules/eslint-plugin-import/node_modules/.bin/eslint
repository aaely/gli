#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/5.6.0/node_modules/eslint/bin/eslint.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/5.6.0/node_modules/eslint/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/5.6.0/node_modules/eslint/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/5.6.0/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../eslint/5.6.0/node_modules/eslint/bin/eslint.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/5.6.0/node_modules/eslint/bin/eslint.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/5.6.0/node_modules/eslint/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/5.6.0/node_modules/eslint/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/5.6.0/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/eslint/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../eslint/5.6.0/node_modules/eslint/bin/eslint.js" "$@"
  ret=$?
fi
exit $ret

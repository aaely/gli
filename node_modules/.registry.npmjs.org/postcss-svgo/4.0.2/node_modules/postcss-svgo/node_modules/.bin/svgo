#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/1.3.0/node_modules/svgo/bin/svgo/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/1.3.0/node_modules/svgo/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/1.3.0/node_modules/svgo/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/1.3.0/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../svgo/1.3.0/node_modules/svgo/bin/svgo" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/1.3.0/node_modules/svgo/bin/svgo/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/1.3.0/node_modules/svgo/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/1.3.0/node_modules/svgo/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/1.3.0/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/svgo/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../svgo/1.3.0/node_modules/svgo/bin/svgo" "$@"
  ret=$?
fi
exit $ret

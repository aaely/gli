#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/2.5.2/node_modules/sane/src/cli.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/2.5.2/node_modules/sane/src/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/2.5.2/node_modules/sane/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/2.5.2/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../sane/2.5.2/node_modules/sane/src/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/2.5.2/node_modules/sane/src/cli.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/2.5.2/node_modules/sane/src/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/2.5.2/node_modules/sane/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/2.5.2/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/sane/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../sane/2.5.2/node_modules/sane/src/cli.js" "$@"
  ret=$?
fi
exit $ret

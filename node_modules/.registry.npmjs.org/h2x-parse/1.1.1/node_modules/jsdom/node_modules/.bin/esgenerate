#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/bin/esgenerate.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/1.12.0/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../escodegen/1.12.0/node_modules/escodegen/bin/esgenerate.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/bin/esgenerate.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/1.12.0/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/escodegen/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../escodegen/1.12.0/node_modules/escodegen/bin/esgenerate.js" "$@"
  ret=$?
fi
exit $ret

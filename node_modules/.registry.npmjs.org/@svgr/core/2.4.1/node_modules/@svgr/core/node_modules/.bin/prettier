#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/prettier/1.18.2/node_modules/prettier/bin-prettier.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/prettier/1.18.2/node_modules/prettier/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/prettier/1.18.2/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/prettier/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../../../prettier/1.18.2/node_modules/prettier/bin-prettier.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/prettier/1.18.2/node_modules/prettier/bin-prettier.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/prettier/1.18.2/node_modules/prettier/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/prettier/1.18.2/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/prettier/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../../../prettier/1.18.2/node_modules/prettier/bin-prettier.js" "$@"
  ret=$?
fi
exit $ret

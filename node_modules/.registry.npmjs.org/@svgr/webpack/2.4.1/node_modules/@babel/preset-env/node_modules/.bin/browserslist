#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/browserslist/4.7.0/node_modules/browserslist/cli.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/browserslist/4.7.0/node_modules/browserslist/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/browserslist/4.7.0/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/browserslist/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../../../browserslist/4.7.0/node_modules/browserslist/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/browserslist/4.7.0/node_modules/browserslist/cli.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/browserslist/4.7.0/node_modules/browserslist/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/browserslist/4.7.0/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/browserslist/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../../../browserslist/4.7.0/node_modules/browserslist/cli.js" "$@"
  ret=$?
fi
exit $ret

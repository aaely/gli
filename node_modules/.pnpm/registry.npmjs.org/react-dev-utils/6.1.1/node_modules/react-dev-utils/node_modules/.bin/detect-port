#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/1.1.6/node_modules/detect-port-alt/bin/detect-port/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/1.1.6/node_modules/detect-port-alt/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/1.1.6/node_modules/detect-port-alt/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/1.1.6/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../detect-port-alt/1.1.6/node_modules/detect-port-alt/bin/detect-port" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/1.1.6/node_modules/detect-port-alt/bin/detect-port/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/1.1.6/node_modules/detect-port-alt/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/1.1.6/node_modules/detect-port-alt/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/1.1.6/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/detect-port-alt/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../detect-port-alt/1.1.6/node_modules/detect-port-alt/bin/detect-port" "$@"
  ret=$?
fi
exit $ret

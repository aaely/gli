#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/bin/esgenerate.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/1.12.0/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../escodegen/1.12.0/node_modules/escodegen/bin/esgenerate.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/bin/esgenerate.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/1.12.0/node_modules/escodegen/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/1.12.0/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/escodegen/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../escodegen/1.12.0/node_modules/escodegen/bin/esgenerate.js" "$@"
  ret=$?
fi
exit $ret

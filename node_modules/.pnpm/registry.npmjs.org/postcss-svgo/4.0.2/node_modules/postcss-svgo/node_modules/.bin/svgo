#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/1.3.2/node_modules/svgo/bin/svgo/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/1.3.2/node_modules/svgo/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/1.3.2/node_modules/svgo/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/1.3.2/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../svgo/1.3.2/node_modules/svgo/bin/svgo" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/1.3.2/node_modules/svgo/bin/svgo/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/1.3.2/node_modules/svgo/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/1.3.2/node_modules/svgo/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/1.3.2/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/svgo/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../svgo/1.3.2/node_modules/svgo/bin/svgo" "$@"
  ret=$?
fi
exit $ret

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/9.7.1/node_modules/autoprefixer/bin/autoprefixer/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/9.7.1/node_modules/autoprefixer/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/9.7.1/node_modules/autoprefixer/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/9.7.1/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../autoprefixer/9.7.1/node_modules/autoprefixer/bin/autoprefixer" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/9.7.1/node_modules/autoprefixer/bin/autoprefixer/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/9.7.1/node_modules/autoprefixer/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/9.7.1/node_modules/autoprefixer/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/9.7.1/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/autoprefixer/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../autoprefixer/9.7.1/node_modules/autoprefixer/bin/autoprefixer" "$@"
  ret=$?
fi
exit $ret

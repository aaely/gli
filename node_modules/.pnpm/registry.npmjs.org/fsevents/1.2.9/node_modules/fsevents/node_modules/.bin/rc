#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/rc/cli.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/rc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../rc/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/rc/cli.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/rc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/fsevents/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../rc/cli.js" "$@"
  ret=$?
fi
exit $ret

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/0.5.5/node_modules/image-size/bin/image-size.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/0.5.5/node_modules/image-size/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/0.5.5/node_modules/image-size/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/0.5.5/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../image-size/0.5.5/node_modules/image-size/bin/image-size.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/0.5.5/node_modules/image-size/bin/image-size.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/0.5.5/node_modules/image-size/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/0.5.5/node_modules/image-size/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/0.5.5/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/image-size/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../image-size/0.5.5/node_modules/image-size/bin/image-size.js" "$@"
  ret=$?
fi
exit $ret

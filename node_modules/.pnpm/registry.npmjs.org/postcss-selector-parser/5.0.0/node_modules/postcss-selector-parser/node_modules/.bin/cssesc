#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/2.0.0/node_modules/cssesc/bin/cssesc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/2.0.0/node_modules/cssesc/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/2.0.0/node_modules/cssesc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/2.0.0/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../cssesc/2.0.0/node_modules/cssesc/bin/cssesc" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/2.0.0/node_modules/cssesc/bin/cssesc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/2.0.0/node_modules/cssesc/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/2.0.0/node_modules/cssesc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/2.0.0/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/cssesc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../cssesc/2.0.0/node_modules/cssesc/bin/cssesc" "$@"
  ret=$?
fi
exit $ret

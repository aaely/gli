#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/2.9.4_webpack@3.8.1/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/2.9.4_webpack@3.8.1/node_modules/webpack-dev-server/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/2.9.4_webpack@3.8.1/node_modules/webpack-dev-server/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/2.9.4_webpack@3.8.1/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../webpack-dev-server/2.9.4_webpack@3.8.1/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/2.9.4_webpack@3.8.1/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/2.9.4_webpack@3.8.1/node_modules/webpack-dev-server/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/2.9.4_webpack@3.8.1/node_modules/webpack-dev-server/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/2.9.4_webpack@3.8.1/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../webpack-dev-server/2.9.4_webpack@3.8.1/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
  ret=$?
fi
exit $ret

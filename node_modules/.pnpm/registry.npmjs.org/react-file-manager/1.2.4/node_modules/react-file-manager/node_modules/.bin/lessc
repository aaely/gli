#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/2.7.3/node_modules/less/bin/lessc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/2.7.3/node_modules/less/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/2.7.3/node_modules/less/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/2.7.3/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../less/2.7.3/node_modules/less/bin/lessc" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/2.7.3/node_modules/less/bin/lessc/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/2.7.3/node_modules/less/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/2.7.3/node_modules/less/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/2.7.3/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/less/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../less/2.7.3/node_modules/less/bin/lessc" "$@"
  ret=$?
fi
exit $ret

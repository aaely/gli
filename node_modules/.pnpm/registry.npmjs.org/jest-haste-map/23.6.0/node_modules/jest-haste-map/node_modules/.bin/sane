#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/2.5.2/node_modules/sane/src/cli.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/2.5.2/node_modules/sane/src/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/2.5.2/node_modules/sane/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/2.5.2/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../sane/2.5.2/node_modules/sane/src/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/2.5.2/node_modules/sane/src/cli.js/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/2.5.2/node_modules/sane/src/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/2.5.2/node_modules/sane/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/2.5.2/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/sane/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../sane/2.5.2/node_modules/sane/src/cli.js" "$@"
  ret=$?
fi
exit $ret

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/bin/uglifyjs/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../terser/3.17.0/node_modules/terser/bin/uglifyjs" "$@"
  ret=$?
else 
  NODE_PATH="/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/bin/uglifyjs/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/bin/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/terser/node_modules:/home/aely/gli/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aely/gli/node_modules/.pnpm/node_modules:/home/aely/gli/node_modules:/home/aely/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../terser/3.17.0/node_modules/terser/bin/uglifyjs" "$@"
  ret=$?
fi
exit $ret

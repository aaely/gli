#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/esprima/bin/esvalidate.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/esprima/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/esprima/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/aaron_e/gli/node_modules/esprima/bin/esvalidate.js/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/esprima/bin/node_modules:/mnt/c/Users/aaron_e/gli/node_modules/esprima/node_modules:/mnt/c/Users/aaron_e/gli/node_modules:/mnt/c/Users/aaron_e/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../esprima/bin/esvalidate.js" "$@"
  ret=$?
fi
exit $ret
